{
    theme: {
        results: {
            backdrop: { // optional central area backdrop
                key: 'results.backdrop', // asset key
                alpha: 1,
            },
            rows: [ // an array of rows
                {
                    format: [ // each object in this array generates a visible object in the row
                        {
                            type: 'text',
                            content: 'Results Screen',
                            offsetX: 0, // offset the x position of this text object
                            offsetY: -18, // offset the y position of this text object
                            textStyle: {
                                fontFamily: 'ReithSans',
                                fontSize: '24px', // Font size should be a minimum of 13px as per GEL
                                color: '#FFFFFF', // This should meet the colour contrast requirement as per GEL
                            },
                        },
                    ],
                },
                {
                    format: [
                        {
                            type: 'text',
                            content: 'You collected: ',
                            offsetY: -20,
                            textStyle: {
                                fontFamily: 'ReithSans',
                                fontSize: '24px',
                                color: '#FFFFFF',
                            },
                        },
                        { type: 'sprite', key: 'game.star', offsetX: 20, offsetY: -20 },
                        {
                            type: 'countup',
                            startCount: 0, // the number to start counting from
                            endCount: '<%= stars %>', // the number to count up to
                            startDelay: 1000, // delay before the countup starts (in ms)
                            countupDuration: 1000, // how long the countup should last (in ms)
                            audio: {
                                key: 'results.coin-sfx',
                                singleTicksRange: 10, // will play audio every text update below this number, otherwise will play x ticksPerSecond.
                                ticksPerSecond: 6, // fire x times in a second - if not defined, will play every text update.
                                startPlayRate: 0.8, // initial play rate - used for pitch shifting
                                endPlayRate: 1.2, // final play rate - used for pitch shifting
                            },
                            offsetX: 15,
                            offsetY: -20,
                            textStyle: {
                                fontFamily: 'ReithSans',
                                fontSize: '24px',
                                color: '#FFFF00',
                                align: 'center',
                            },
                        },
                        {
                            type: 'text',
                            content: ' star<% stars !== 1 && print("s") %>',
                            offsetY: -20,
                            textStyle: {
                                fontFamily: 'ReithSans',
                                fontSize: '24px',
                                color: '#FFFF00',
                            },
                        },
                        {
                            type: 'text',
                            content: '!',
                            offsetY: -20,
                            textStyle: {
                                fontFamily: 'ReithSans',
                                fontSize: '24px',
                                color: '#FFFFFF',
                            },
                        },
                    ],
                    backdrop: { // optional row backdrop
                        key: 'results.row-backdrop-1', // asset key
                        alpha: 0.5,
                        offsetY: -20,
                    },
                    alpha: 1, // set the alpha on the whole row
                    transition: { rotation: { from: -4, to: 0 }, alpha: { from: 0, to: 1 }, duration: 1000 }, // phaser tweendata config
                    audio: { key: 'results.woosh', delay: 0 }, // asset key and delay for audio to be played
                },
                {
                    format: [
                        {
                            type: 'text',
                            content: 'You collected: ',
                            offsetY: -20,
                            textStyle: {
                                fontFamily: 'ReithSans',
                                fontSize: '24px',
                                color: '#FFFFFF',
                            },
                        },
                        {
                            type: 'sprite',
                            key: 'game.diamond', // asset key of spritesheet
                            frame: 0, // the initial frame of the sprite to display
                            offsetX: 20,
                            offsetY: -20,
                            anim: { // a phaser animation configuration object
                                frames: {
                                    start: 0,
                                    end: 14,
                                },
                                frameRate: 7,
                                repeat: -1,
                            },
                        },
                        {
                            type: 'countup',
                            startCount: 0,
                            endCount: '<%= gems %>',
                            startDelay: 4000,
                            countupDuration: 1000,
                            audio: {
                                key: 'results.gem-sfx',
                                singleTicksRange: 10,
                                ticksPerSecond: 6,
                            },
                            offsetX: 15,
                            offsetY: -20,
                            textStyle: {
                                fontFamily: 'ReithSans',
                                fontSize: '24px',
                                color: '#00FF00',
                                align: 'center',
                            },
                        },
                        {
                            type: 'text',
                            content: ' gem<% gems !== 1 && print("s") %>',
                            offsetY: -20,
                            textStyle: {
                                fontFamily: 'ReithSans',
                                fontSize: '24px',
                                color: '#00FF00',
                            },
                        },
                        {
                            type: 'text',
                            content: '!',
                            offsetY: -20,
                            textStyle: {
                                fontFamily: 'ReithSans',
                                fontSize: '24px',
                                color: '#FFFFFF',
                            },
                        },
                    ],
                    backdrop: {
                        key: 'results.row-backdrop-2',
                        alpha: 0.5,
                        offsetY: -20,
                    },
                    alpha: 0,
                    transition: {
                        alpha: {
                            from: 0,
                            to: 1,
                        },
                        x: {
                            from: -200,
                            to: 0,
                        },
                        duration: 2000,
                        delay: 2000,
                    },
                    audio: { key: 'results.slide', delay: 2500 },
                },
                {
                    format: [
                        {
                            type: 'text',
                            content: 'You collected: ',
                            offsetY: -20,
                            textStyle: {
                                fontFamily: 'ReithSans',
                                fontSize: '24px',
                                color: '#FFFFFF',
                            },
                        },
                        { type: 'sprite', key: 'game.key', offsetX: 0, offsetY: -20 },
                        {
                            type: 'countup',
                            startCount: 0,
                            endCount: '<%= keys %>',
                            startDelay: 7000,
                            countupDuration: 1000,
                            audio: {
                                key: 'results.key-sfx',
                                singleTicksRange: 10,
                                ticksPerSecond: 6,
                            },
                            offsetX: 15,
                            offsetY: -20,
                            textStyle: {
                                fontFamily: 'ReithSans',
                                fontSize: '24px',
                                color: '#FFA500',
                                align: 'center',
                            },
                        },
                        {
                            type: 'text',
                            content: ' key<% keys !== 1 && print("s") %>',
                            offsetY: -20,
                            textStyle: {
                                fontFamily: 'ReithSans',
                                fontSize: '24px',
                                color: '#FFA500',
                            },
                        },
                        {
                            type: 'text',
                            content: '!',
                            offsetY: -20,
                            textStyle: {
                                fontFamily: 'ReithSans',
                                fontSize: '24px',
                                color: '#FFFFFF',
                            },
                        },
                    ],
                    backdrop: {
                        key: 'results.row-backdrop-3',
                        alpha: 0.5,
                        offsetY: -20,
                    },
                    alpha: 0,
                    transition: {
                        scale: { from: 0, to: 1 },
                        alpha: 1,
                        duration: 2000,
                        ease: 'Elastic.Out',
                        delay: 5500,
                    },
                    audio: { key: 'results.boing', delay: 5500 },
                },
            ],
        },
    },
}
