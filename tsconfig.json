{
    "compilerOptions": {
        "module": "CommonJS",
        "target": "es5",
        "strict": true,
        "sourceMap": true,
        "baseUrl": ".",
        "typeRoots": ["./src/types", "./node_modules/@types"]
    },
    "awesomeTypescriptLoaderOptions": {
        "useBabel": true,
        "babelOptions": {
            "babelrc": false,
            "presets": [["env", {"targets": {"ie": 11}}]],
            "plugins": "babel-polyfill"
        },
        "babelCore": "babel-core"
    }
}
